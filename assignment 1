# !/bin/bash
# Create a File 
touch rounak.txt

# Assign Permission
chmod 700 rounak.txt # read ,write ,excute for Owner
chmod 750 rounak.txt # read ,write ,excute for Owner ; read ,excute for group
chmod 755 rounak.txt # read ,write ,excute for Owner ; read ,excute for group ; read ,excute for user

ls -l rounak.txt
